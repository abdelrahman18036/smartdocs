{
  "name": "smartdocs",
  "version": "0.1.0-beta.0",
  "description": "Smart documentation generator for React/Next",
  "type": "module",
  "bin": { "smartdocs": "./dist/cli.js" },
  "exports": {
    ".": "./dist/cli.js",
    "./config": "./dist/config.js"
  },
  "files": ["dist", "templates", "readme.md"],
  "engines": { "node": ">=18" },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/abdelrahman18036/smartdocs"
  },
  "homepage": "https://github.com/abdelrahman18036/smartdocs",
  "bugs": {
    "url": "https://github.com/abdelrahman18036/smartdocs/issues"
  },
  "scripts": {
    "build": "tsup src/cli.ts src/config.ts --format esm --dts --out-dir dist",
    "dev": "tsx watch src/cli.ts",
    "test": "node dist/cli.js --help",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "commander": "^12",
    "globby": "^14",
    "picocolors": "^1",
    "ora": "^8",
    "react-docgen-typescript": "^2",
    "react-docgen": "^7",
    "@babel/parser": "^7",
    "zod": "^3",
    "chokidar": "^3"
  },
  "devDependencies": {
    "tsup": "^8",
    "tsx": "^4",
    "typescript": "^5",
    "@types/node": "^20"
  }
}
